{
  "name": "DeclarativeNetRequest Issues",
  "version": "1.0.0",
  "description": "Example Extension highlighting issues in declarativeNetRequest API",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": ["*://*.example.com/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
